{
  "compilerOptions": {
    "baseUrl": "../",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "module": "CommonJS",
    "target": "ES2016",
    "moduleResolution": "Node",
    "downlevelIteration": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "resolveJsonModule": true,
    "paths": {
      "@gym/*": [
        "../packages/*/src"
      ]
    },
  }
}
